test PrimaryTest {
  assert 1 == 1;
  assert (1 != 2);
  assert !(1 == 2);
  assert 1 < 2;
  assert 1 <= 2;
  assert !(1 > 2);
  assert !(1 >= 2);
  assert 2 > 1;
  assert 2 >= 1;
  assert !(2 < 1);
  assert !(2 <= 1);
  assert 1 < 3.14;
  assert 1 <= 3.14;
  assert !(1 > 3.14);
  assert !(1 >= 3.14);
  assert 3.14 > 1;
  assert 3.14 >= 1;
  assert !(3.14 < 1);
  assert !(3.14 <= 1);
  assert 2.71 < 3.14;
  assert 2.71 <= 3.14;
  assert !(2.71 > 3.14);
  assert !(2.71 >= 3.14);
  assert 3.14 > 2.71;
  assert 3.14 >= 2.71;
  assert !(3.14 < 2.71);
  assert !(3.14 <= 2.71);
}
